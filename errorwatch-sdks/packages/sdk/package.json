{
  "name": "@errorwatch/sdk",
  "version": "1.0.0",
  "type": "module",
  "description": "Universal error monitoring SDK for browser, React, and Vue applications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/integrations/react.d.ts",
      "import": "./dist/integrations/react.js"
    },
    "./vue": {
      "types": "./dist/integrations/vue.d.ts",
      "import": "./dist/integrations/vue.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "rrweb": "^1.1.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0 || ^19.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "error-monitoring",
    "error-tracking",
    "sentry-alternative",
    "react",
    "vue",
    "browser"
  ],
  "license": "MIT"
}
